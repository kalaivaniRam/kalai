      <ProgramUnit Name="SHOW_CODES_LOV" ProgramUnitText="FUNCTION SHOW_CODES_LOV (&amp;#10;  p_lov_name&#x9;&#x9;&#x9;varchar2, &#x9; -- name of lov&amp;#10;  p_description&#x9;&#x9;in out&#x9;varchar2, &#x9; -- description&amp;#10;  p_user_code&#x9;&#x9;in out&#x9;varchar2,   &#x9; -- user_code&amp;#10;  p_code&#x9;&#x9;in out&#x9;number,&#x9;  &#x9; -- code&#x9;&amp;#10;  p_code_type&#x9;&#x9;&#x9;number,&#x9;&#x9; -- code_type from lov&amp;#10;  p_use_code_type_parameter&#x9;boolean  := FALSE, -- pass TRUE if lov uses :PARAMETER.P_LOV_CODE_TYPE&amp;#10;-- *** the following arguments are here to allow us to set the items to change allowed.&amp;#10;  p_desc_item_name&#x9;&#x9;varchar2 := '*', -- name of description item &amp;#10;  p_user_code_item_name&#x9;&#x9;varchar2 := '*' -- name of user_code item&amp;#10;) RETURN boolean IS&amp;#10;--&amp;#10;-- show codes_lov &amp; return values, &amp;#10;-- call form addcode if user selects create new record option from lov&amp;#10;-- &amp;#10;-- return TRUE if a record is selected&amp;#10;--&amp;#10;-- please ensure that the following 3 parameters are in the form&amp;#10;--  P_LOV_CODE&amp;#10;--  P_LOV_USER_CODE&amp;#10;--  P_LOV_DESCRIPTION&amp;#10;--&amp;#10;-- use a select in the following format in your lov&amp;#10;--   &amp;#10;--   select description, user_code, code&amp;#10;--   from codes&amp;#10;--   where code_type = 10000 and prog_code = 1&amp;#10;--   UNION&amp;#10;--   select description, user_code, code&amp;#10;--   from codes&amp;#10;--   where code_type = :PARAMETER.P_LOV_CODE_TYPE&amp;#10;--&amp;#10;-- date&#x9;&#x9;by&#x9;&#x9;change&amp;#10;-- 07/08/1996&#x9;Steve Pusey&#x9;Creation&amp;#10;--&amp;#10;-- 10/09/1996&#x9;Steve Pusey&#x9;Change title on LOV (use code_types.description)&amp;#10;--&#x9;&#x9;&#x9;&#x9;Added code to handle NULL option&amp;#10;--&amp;#10;&amp;#10;  v_temp_code&#x9;&#x9;varchar2(30);&amp;#10;  v_rec_selected&#x9;boolean := FALSE;&amp;#10;  v_title&#x9;&#x9;varchar2(80); -- title for lov&amp;#10;&amp;#10;  cursor c_lov_title is&amp;#10;    select description&amp;#10;    from code_types&amp;#10;    where code_type = p_code_type;&amp;#10;&amp;#10;-- ############################## INTERNAL PROGRAM UNITS ###############################&amp;#10;&amp;#10;  procedure set_items (&amp;#10;    p_property&#x9;&#x9;number, -- PROPERTY_ON or PROPERTY_OFF&amp;#10;    p_item_name_1&#x9;varchar2,&amp;#10;    p_item_name_2&#x9;varchar2&amp;#10;  ) is&amp;#10;  BEGIN&amp;#10;&amp;#10;    if p_item_name_1 != '*' then&amp;#10;      set_item_property (p_item_name_1, UPDATE_ALLOWED, p_property);&amp;#10;      set_item_property (p_item_name_1, INSERT_ALLOWED, p_property);&amp;#10;&amp;#10;    end if;&amp;#10;&amp;#10;    if p_item_name_2 != '*' then&amp;#10;      set_item_property (p_item_name_2, UPDATE_ALLOWED, p_property);&amp;#10;      set_item_property (p_item_name_2, INSERT_ALLOWED, p_property);&amp;#10;&amp;#10;    end if;&amp;#10;&amp;#10;  END set_items;&amp;#10;&amp;#10;-- #####################################################################################&amp;#10; &amp;#10;BEGIN&amp;#10;  &amp;#10;  -- set items to change allowed&amp;#10;  set_items (PROPERTY_ON, p_desc_item_name, p_user_code_item_name);&amp;#10;&amp;#10;  -- if p_use_code_type_parameter then set :PARAMETER.P_LOV_CODE_TYPE&amp;#10;  if p_use_code_type_parameter then&amp;#10;&amp;#10;    copy (to_char(p_code_type), 'PARAMETER.P_LOV_CODE_TYPE');&amp;#10;&amp;#10;    -- set lov_title&amp;#10;&amp;#10;    open c_lov_title;&amp;#10;    fetch c_lov_title into v_title;&amp;#10;    close c_lov_title;&amp;#10;&amp;#10;    set_lov_property (p_lov_name, TITLE, v_title);&amp;#10;&amp;#10;  end if;&amp;#10;&amp;#10;  -- show lov&amp;#10;  if show_lov (p_lov_name) then&amp;#10;&amp;#10;    v_rec_selected := TRUE;&amp;#10;&amp;#10;    if name_in('PARAMETER.P_LOV_USER_CODE') = '*NEW' then&amp;#10;      -- add a new code to p_code_type&amp;#10;      call_form_addcode (p_code_type, p_code, p_user_code, p_description);&amp;#10;&amp;#10;    elsif name_in('PARAMETER.P_LOV_USER_CODE') = 'NULL' then&amp;#10;      -- set items to NULL&amp;#10;&amp;#10;      p_code := null;&amp;#10;      p_user_code := null;&amp;#10;      p_description := null;&amp;#10;&amp;#10;    else&amp;#10;      -- put values from lov into items&amp;#10;&amp;#10;      v_temp_code :=  name_in('PARAMETER.P_LOV_CODE');&amp;#10;      p_code := v_temp_code;&amp;#10;&amp;#10;      p_user_code := name_in('PARAMETER.P_LOV_USER_CODE');&amp;#10;      p_description := name_in('PARAMETER.P_LOV_DESCRIPTION');&amp;#10;&amp;#10;    end if;&amp;#10;&amp;#10;  end if;&amp;#10;&amp;#10;  -- set items to no change allowed&amp;#10;  set_items (PROPERTY_OFF, p_desc_item_name, p_user_code_item_name);&amp;#10;&amp;#10;  return v_rec_selected;&amp;#10;&amp;#10;END show_codes_lov;" ProgramUnitType="Function"/>
      <ProgramUnit Name="VALIDATE_ADTSEARCHGROUP_ID" ProgramUnitText="FUNCTION validate_adtsearchgroup_id (&amp;#10;  p_searchgroup_id&#x9;&#x9;&#x9;&#x9;&#x9;in&#x9;&#x9;&#x9;number,&amp;#10;  p_searchgroup_usercode&#x9;&#x9;IN OUT&#x9;varchar2,&amp;#10;  p_searchgroup_desc&#x9;&#x9;&#x9;&#x9;IN OUT&#x9;varchar2,&amp;#10;  p_validate&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;in&#x9;&#x9;&#x9;varchar2 DEFAULT 'VALIDATE'&amp;#10;)  RETURN boolean IS&amp;#10;--&amp;#10;-- validate ADT searchgroup id&amp;#10;--&amp;#10;-- date&#x9;&#x9;&#x9;by&#x9;&#x9;change&amp;#10;-- 26/01/2000&#x9;&#x9;Steve&#x9;&#x9;Created&amp;#10;--&amp;#10;&amp;#10;  v_curr_user_id&#x9;number&#x9;DEFAULT to_number(name_in ('GLOBAL.USER_ID'));&amp;#10;  v_found&#x9;&#x9;boolean;&amp;#10;&amp;#10;  cursor c_valid is&amp;#10;  &#x9;select user_code, description&amp;#10;  &#x9;from adtentity_searchgroups&amp;#10;  &#x9;where adtentity_searchgroup_id = p_searchgroup_id;&amp;#10;&amp;#10;BEGIN&amp;#10;  &amp;#10;  open c_valid;&amp;#10;  fetch c_valid into p_searchgroup_usercode, p_searchgroup_desc;&amp;#10;  v_found := c_valid%FOUND;&amp;#10;  close c_valid;&amp;#10;  &amp;#10;  IF not v_found&amp;#10;    and p_validate = 'VALIDATE'&amp;#10;  THEN&amp;#10;  &amp;#10;  &#x9;display_alert ('ERROR_ALERT', 684); -- invalid code, please re-enter&amp;#10;  &#x9;RAISE FORM_TRIGGER_FAILURE;&amp;#10;  &#x9;&amp;#10;  END IF;&amp;#10;  &amp;#10;  RETURN v_found;&amp;#10;  &amp;#10;END;&amp;#10;" ProgramUnitType="Function"/>
