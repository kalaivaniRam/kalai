--
-- validate the short code
--
-- date		by		change
-- 26/01/2000	Steve		Created
--
DECLARE

  v_ok			boolean;

BEGIN

  IF :C1.WARD_USER_CODE is NULL THEN

	:C1.WARD_DESC := null;
	:C1.WORK_ENTITY := null;

  ELSE

	v_ok := validate_work_entity_code (
	 	:C1.WARD_USER_CODE,
		:C1.WORK_ENTITY,
		:C1.WARD_DESC,
		'VALIDATE',
		0, -- current hospital
		'W' -- wards
	);

  END IF;

END;			
