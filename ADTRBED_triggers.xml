FUNCTION validate_adtsearchgroup_id (
  p_searchgroup_id					in			number,
  p_searchgroup_usercode		IN OUT	varchar2,
  p_searchgroup_desc				IN OUT	varchar2,
  p_validate								in			varchar2 DEFAULT 'VALIDATE'
)  RETURN boolean IS
--
-- validate ADT searchgroup id
--
-- date			by		change
-- 26/01/2000		Steve		Created
--

  v_curr_user_id	number	DEFAULT to_number(name_in ('GLOBAL.USER_ID'));
  v_found		boolean;

  cursor c_valid is
  	select user_code, description
  	from adtentity_searchgroups
  	where adtentity_searchgroup_id = p_searchgroup_id;

BEGIN
  
  open c_valid;
  fetch c_valid into p_searchgroup_usercode, p_searchgroup_desc;
  v_found := c_valid%FOUND;
  close c_valid;
  
  IF not v_found
    and p_validate = 'VALIDATE'
  THEN
  
  	display_alert ('ERROR_ALERT', 684); -- invalid code, please re-enter
  	RAISE FORM_TRIGGER_FAILURE;
  	
  END IF;
  
  RETURN v_found;
  
END;

