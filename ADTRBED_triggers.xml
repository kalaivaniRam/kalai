PACKAGE BODY init_form IS
--
-- steve pusey 2/5/1996
--
-- program units to initialise form
--
-- add code to init_form and init_format_masks program units, add other init procs to the
-- init_form procedure, for example if you write a procedure called init_toolbar then add 
-- this to the init_form procedure. The init_form procedure is called from the 
-- WHEN-NEW-FORM-INSTANCE trigger
--

---------------------------------------------------------------------------------------
-- INTERNAL PACKAGE VARIABLES 
---------------------------------------------------------------------------------------

---------------------------------------------------------------------------------------
-- INTERNAL PROGRAM UNITS (DECLARATION)
---------------------------------------------------------------------------------------

  procedure init_list_items;
  procedure init_format_masks;

---------------------------------------------------------------------------------------
-- PROGRAM UNITS (CODE)
---------------------------------------------------------------------------------------

-- ##########################################################################################

  procedure init_list_items is
  --
  -- initialise list items
  --
  -- date	by		change
  -- 2/5/1996   Steve Pusey	Creation of template procedure
  -- 24/01/03   Robert      Added populate of work_id_types  NSPEC337
  --
  BEGIN

    -- cut & paste this code
       populate_poplist_from_codes  ('c1.check1', 10008);
       Populate_Poplist_Work_Ids ('C1.WORK_ID_TYPES');                -- RB 24/01/03 NSPEC337

  END init_list_items;

-- ##########################################################################################

  procedure init_format_masks is
  --
  -- set the format masks for date, datetime, and numeric items
  --
  -- date	by		change
  -- 2/5/1996   Steve Pusey	Creation of template procedure
  --
  vd_sysdate DATE:=sysdate;
  BEGIN

    -- copy & paste this line
    -- set_format_mask ('BLOCK.ITEM');
       set_format_mask ('C1.RUN_DATE');
       :C1.RUN_DATE := vd_SYSDATE;
       :C1.RUN_TIME := vd_SYSDATE;
    
   
  END init_format_masks;

-- ##########################################################################################

  procedure init_form is
  --
  -- initialise form
  --
  -- date	by		change
  -- 2/5/1996   Steve Pusey	Creation of template procedure
  -- 24/01/03   Robert      Added hide of work_id_types  NSPEC337
  -- 30/01/03   Robert      Corrected boilerplate hidden for NSPEC337
  -- 09/12/03   Paul        Ensure the WVI on the site_code does not fire 
  --                        until the user changes it
  --
  BEGIN

    init_list_items;
    init_format_masks;

	--	oasis_char_set_item_property ('C1.CREATE_PROFILE_BTN', LABEL, :TEXT.BPT_BTN_CREATEPROFILE);
	--	oasis_char_set_item_property ('C1.UPDATE_PROFILE_BTN', LABEL, :TEXT.BPT_BTN_UPDATEPROFILE);

		set_tab_page_property ('PROFILES', LABEL, :TEXT.BPT_TAB_PROFILE);
		set_tab_page_property ('DETAILS', LABEL, :TEXT.BPT_TAB_DETAILS);
		set_tab_page_property ('OPTIONS', LABEL, :TEXT.BPT_TAB_OPTIONS);  -- Added RB NSPEC337

		set_item_property ('C1.SITE_USER_CODE', ITEM_IS_VALID, PROPERTY_TRUE);

		-- get the default profile for this user if there is one
		:C1.OASIS_DEFAULT_MASTER_ID := get_default_profile (
			to_number (:GLOBAL.USER_ID),
			to_number (:GLOBAL.FUNCTION_ID),
			'ADTRBEDALL',
			:C1.PROFILE_USER_CODE,
			:C1.PROFILE_DESC,
			'NONE' -- no options
		);

		IF :C1.OASIS_DEFAULT_MASTER_ID is NOT NULL THEN
			
			adtrbed_get_profile (
				:C1.OASIS_DEFAULT_MASTER_ID,
				:C1.PROFILE_USER_CODE,
				:C1.PROFILE_DESC
			);
			
		ELSE
			
			:SYSTEM.MESSAGE_LEVEL := 10; -- suppress 'list contain no values...'
			
			IF show_lov ('OASIS_DEFAULTS_LOV') THEN
			-- user has selected a profile
			
				adtrbed_get_profile (
					:C1.OASIS_DEFAULT_MASTER_ID,
					:C1.PROFILE_USER_CODE,
					:C1.PROFILE_DESC
				);
				
			ELSE
				-- user has no profiles or has cancelled lov, goto 2nd tab
				go_item ('C1.RUN_DATE'); -- got 2nd tab
			END IF;
			
			:SYSTEM.MESSAGE_LEVEL := 0;
			
		END IF;
		
    IF oaslogon_package.get_departmental_id_type_code = oaslogon_package.get_regist_id_type_code THEN
    	OASIS_NUMBER_SET_ITEM_PROPERTY( 'C1.WORK_ID_TYPES', VISIBLE, PROPERTY_FALSE );
    	OASIS_NUMBER_SET_ITEM_PROPERTY( 'TEXT.BPT_14'      , VISIBLE, PROPERTY_FALSE );
    END IF;
    --
  END init_form;

-- ##########################################################################################

END init_form;
