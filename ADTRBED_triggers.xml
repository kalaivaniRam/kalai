-- 26/01/2000	Steve		Created
--

  v_curr_user_id	number	DEFAULT to_number(name_in ('GLOBAL.USER_ID'));
  v_found		boolean;

  cursor c_valid is
  	select adtentity_searchgroup_id, description
  	from adtentity_searchgroups
  	where user_code = p_searchgroup_usercode
  	  and user_id = decode (p_user_id, -1, user_id, 0, v_curr_user_id, p_user_id);

BEGIN
  
  open c_valid;
  fetch c_valid into p_searchgroup_id, p_searchgroup_desc;
  v_found := c_valid%FOUND;
  close c_valid;
  
  IF not v_found
    and p_validate = 'VALIDATE'
  THEN
  
  	display_alert ('ERROR_ALERT', 684); -- invalid code, please re-enter
  	RAISE FORM_TRIGGER_FAILURE;
  	
  END IF;
  
  RETURN v_found;
  
END;
