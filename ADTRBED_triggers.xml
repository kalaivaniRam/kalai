PROCEDURE adtrbed_save_profile (
  p_oasis_default_master_id			in		number
) IS
--
-- save a profile
--
-- date 			by			change
-- 23/01/2000	Steve		Created
--
BEGIN
	
  save_date_default_value (
	  p_oasis_default_master_id,
	  'C1.RUN_DATE',
	  :C1.RUN_DATE
	);

  save_date_default_value (
	  p_oasis_default_master_id,
	  'C1.RUN_TIME',
	  :C1.RUN_TIME
	);

  save_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.ADTENTITY_SEARCHGROUP_ID',
	  to_char(:C1.ADTENTITY_SEARCHGROUP_ID)
	);

  save_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.WORK_ENTITY',
	  to_char(:C1.WORK_ENTITY)
	);

  save_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.CHECK1',
	  :C1.CHECK1
	);

  save_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.CALC_TOTALS_FLAG',
	  :C1.CALC_TOTALS_FLAG
	);

	-- JB 20/02/04 Added visible check
	IF GET_ITEM_PROPERTY('C1.WORK_ID_TYPES', VISIBLE) = 'TRUE' THEN
	  -- Added RB 27/01/03 NSPEC337 -- Added patient id type selection
	  save_nondate_default_value (
	  	p_oasis_default_master_id,
	  	'C1.WORK_ID_TYPES',
	  	:C1.WORK_ID_TYPES
		);
	ELSE
	  save_nondate_default_value (
	  	p_oasis_default_master_id,
	  	'C1.WORK_ID_TYPES',
	  	'RAISED'
		);
	END IF;

  --
  -- Added RB 14/07/03
  --
  save_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.SITE_CODE',
	  :C1.SITE_CODE
	);
	
	:SYSTEM.MESSAGE_LEVEL := 10;
	commit;
	:SYSTEM.MESSAGE_LEVEL := 0;

END;
