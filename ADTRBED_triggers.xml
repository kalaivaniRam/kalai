-- 1995		Nagy Tharwat	Upgrade to v9 - moved code into library procedure
-- 2/5/1996	Steve Pusey	Upgrade to V10 
--				- versions into oasis_new_form_instance
--				- Tidied code and added comments
--

DECLARE

  v_error_no             number(4); -- returned from oasis_new_form_instance

BEGIN

  -- version 2 form (gets bpt and window titles fromm files)
  oasis_new_form_instance_v2(v_error_no, 2); 

  if v_error_no > 0 then
    display_alert ('ERROR_ALERT', v_error_no);
    exit_form (NO_VALIDATE);
  end if;

-- *** Insert additional code into init_form package ***

  init_form.init_form;
  hci_upgrade.pr_initialise_images;

END;
