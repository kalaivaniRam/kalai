FUNCTION SHOW_CODES_LOV (
  p_lov_name			varchar2, 	 -- name of lov
  p_description		in out	varchar2, 	 -- description
  p_user_code		in out	varchar2,   	 -- user_code
  p_code		in out	number,	  	 -- code	
  p_code_type			number,		 -- code_type from lov
  p_use_code_type_parameter	boolean  := FALSE, -- pass TRUE if lov uses :PARAMETER.P_LOV_CODE_TYPE
-- *** the following arguments are here to allow us to set the items to change allowed.
  p_desc_item_name		varchar2 := '*', -- name of description item 
  p_user_code_item_name		varchar2 := '*' -- name of user_code item
) RETURN boolean IS
--
-- show codes_lov & return values, 
-- call form addcode if user selects create new record option from lov
-- 
-- return TRUE if a record is selected
--
-- please ensure that the following 3 parameters are in the form
--  P_LOV_CODE
--  P_LOV_USER_CODE
--  P_LOV_DESCRIPTION
--
-- use a select in the following format in your lov
--   
--   select description, user_code, code
--   from codes
--   where code_type = 10000 and prog_code = 1
--   UNION
--   select description, user_code, code
--   from codes
--   where code_type = :PARAMETER.P_LOV_CODE_TYPE
--
-- date		by		change
-- 07/08/1996	Steve Pusey	Creation
--
-- 10/09/1996	Steve Pusey	Change title on LOV (use code_types.description)
--				Added code to handle NULL option
--

  v_temp_code		varchar2(30);
  v_rec_selected	boolean := FALSE;
  v_title		varchar2(80); -- title for lov

  cursor c_lov_title is
    select description
    from code_types
    where code_type = p_code_type;

-- ############################## INTERNAL PROGRAM UNITS ###############################

  procedure set_items (
    p_property		number, -- PROPERTY_ON or PROPERTY_OFF
    p_item_name_1	varchar2,
    p_item_name_2	varchar2
  ) is
  BEGIN

    if p_item_name_1 != '*' then
      set_item_property (p_item_name_1, UPDATE_ALLOWED, p_property);
      set_item_property (p_item_name_1, INSERT_ALLOWED, p_property);

    end if;

    if p_item_name_2 != '*' then
      set_item_property (p_item_name_2, UPDATE_ALLOWED, p_property);
      set_item_property (p_item_name_2, INSERT_ALLOWED, p_property);

    end if;

  END set_items;

-- #####################################################################################
 
BEGIN
  
  -- set items to change allowed
  set_items (PROPERTY_ON, p_desc_item_name, p_user_code_item_name);

  -- if p_use_code_type_parameter then set :PARAMETER.P_LOV_CODE_TYPE
  if p_use_code_type_parameter then

    copy (to_char(p_code_type), 'PARAMETER.P_LOV_CODE_TYPE');

    -- set lov_title

    open c_lov_title;
    fetch c_lov_title into v_title;
    close c_lov_title;

    set_lov_property (p_lov_name, TITLE, v_title);

  end if;

  -- show lov
  if show_lov (p_lov_name) then

    v_rec_selected := TRUE;

    if name_in('PARAMETER.P_LOV_USER_CODE') = '*NEW' then
      -- add a new code to p_code_type
      call_form_addcode (p_code_type, p_code, p_user_code, p_description);

    elsif name_in('PARAMETER.P_LOV_USER_CODE') = 'NULL' then
      -- set items to NULL

      p_code := null;
      p_user_code := null;
      p_description := null;

    else
      -- put values from lov into items

      v_temp_code :=  name_in('PARAMETER.P_LOV_CODE');
      p_code := v_temp_code;

      p_user_code := name_in('PARAMETER.P_LOV_USER_CODE');
      p_description := name_in('PARAMETER.P_LOV_DESCRIPTION');

    end if;

  end if;

  -- set items to no change allowed
  set_items (PROPERTY_OFF, p_desc_item_name, p_user_code_item_name);

  return v_rec_selected;

END show_codes_lov;
