-- Check if the user code is correnct.
--
--Date        By      Change
--14/07/2003  ROB     Creation
--
DECLARE
  --
  cursor c1 is
  	select description, code
  	from   codes
  	where  code_type = 10454
  	and    user_code = :c1.site_user_code;
  --
BEGIN
  if :system.mode = 'NORMAL' then
    if :c1.site_user_code IS NOT NULL then
      open c1;
      fetch c1 into :c1.site_name, :c1.site_code;
      if c1%NOTFOUND then
        close c1;
        :c1.site_name := null;
        :c1.site_code := null;
        display_alert('error_alert',1815);
        raise form_trigger_failure;
      end if;
    else
      :c1.site_name := null;
      :c1.site_code := null;
    end if;
    :c1.ward_user_code := null;
    :c1.ward_desc      := null;
    :c1.work_entity    := null;
  end if;
END;
