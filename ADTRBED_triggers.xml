PROCEDURE adtrbed_get_profile (
  p_oasis_default_master_id			in		number,
  p_profile_user_code						in		varchar2,
  p_profile_desc								in		varchar2
) IS
--
-- get defaults from a profile
--
-- date 			by			change
-- 24/01/2000	Steve		Created
-- 27/01/2003 Robert  Added patient id type parameter
--

  v_ok						boolean;
	v_user_code			varchar2(15);

BEGIN
	
  go_block ('C1');
  clear_block (NO_VALIDATE);
  
  :C1.PROFILE_DESC := p_profile_desc;
  :C1.PROFILE_USER_CODE := p_profile_user_code;
  :C1.OASIS_DEFAULT_MASTER_ID := p_oasis_default_master_id;
  
  :C1.RUN_DATE := get_date_default_value (
	  p_oasis_default_master_id,
	  'C1.RUN_DATE'
	);

  :C1.RUN_TIME := get_date_default_value (
	  p_oasis_default_master_id,
	  'C1.RUN_TIME'
	);

	:C1.ADTENTITY_SEARCHGROUP_ID := to_number(get_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.ADTENTITY_SEARCHGROUP_ID'
	));

	v_ok := validate_adtsearchgroup_id (
		:C1.ADTENTITY_SEARCHGROUP_ID,
		:C1.SEARCHGROUP_USER_CODE,
		:C1.SEARCHGROUP_DESC,
		'NOVALIDATE'
	);
	
	oasis_number_set_item_property ('C1.SEARCHGROUP_USER_CODE', ITEM_IS_VALID, PROPERTY_ON);

  :C1.WORK_ENTITY := to_number(get_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.WORK_ENTITY'
	));
	
	v_ok := validate_work_entity2 (
		:C1.WORK_ENTITY,
		:C1.WARD_USER_CODE,
		:C1.WARD_DESC,
		'NOVALIDATE'
	);

	oasis_number_set_item_property ('C1.WARD_USER_CODE', ITEM_IS_VALID, PROPERTY_ON);

  :C1.CHECK1 := get_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.CHECK1'
	);

  :C1.CALC_TOTALS_FLAG := get_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.CALC_TOTALS_FLAG'
	);
		
	-- JB 20/02/04 Added visible check
	IF GET_ITEM_PROPERTY('C1.WORK_ID_TYPES', VISIBLE) = 'TRUE' THEN
		-- Added Rb 27/01/03 NSPEC337
	  :C1.WORK_ID_TYPES := get_nondate_default_value (
	  														p_oasis_default_master_id,
	  														'C1.WORK_ID_TYPES'
																										);
	END IF;

	-- Added Rb 17/07/03 
  :C1.site_code := get_nondate_default_value (
	  p_oasis_default_master_id,
	  'C1.SITE_CODE'
	);

	v_ok := validate_code (
		:C1.SITE_CODE,
		:C1.SITE_USER_CODE,
		:C1.SITE_NAME,
		NULL,
		'NOVALIDATE'
	);


END;
