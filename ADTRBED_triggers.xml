/*
--
-- run bed state report
--
-- date		by		change
-- 09/09/1996	Steve Pusey	Creation
-- 27/01/2000	Steve		added run_time, adtentity_searchgroup_id, and calc_totals_flag
-- 17/1/02	Luca	Added add_parameter 'RECURSIVE_LOAD' to stop ORA-6508
-- 27/01/03   Robert  Added parameter 'WORK_ID_TYPE' for passing the of patient identifier to show - NSPEC337
-- 17/07/03   Robert  Added parameter 'SITE_CODE' for passing of the work entity site code
--
DECLARE

  p_list_id	paramlist; -- parameter list id
  v_date	date;
  vd_sysdate DATE:=sysdate;

BEGIN

  validate (FORM_SCOPE); -- validate date formats etc

  if not FORM_SUCCESS then
    raise form_trigger_failure;
  end if;

  if :C1.RUN_DATE is null 
  then
    :C1.RUN_DATE := vd_sysdate;
  end if;

  if :C1.RUN_TIME is null 
  then
    :C1.RUN_TIME := vd_sysdate;
  end if;

    -- check if parameter list already exists - delete it if it does
    p_list_id := get_parameter_list ('BEDSTATE');
    
    if not id_null (p_list_id) then
      destroy_parameter_list ('BEDSTATE');
    end if;

    -- create parameter list from values entered by user  
    p_list_id := create_parameter_list ('BEDSTATE');

    v_date := to_date (to_char(:C1.RUN_DATE, 'DD-MM-YYYY')||' '||to_char(:C1.RUN_TIME, 'HH24:MI'), 'DD-MM-YYYY HH24:MI');

		add_parameter (p_list_id, 'P_RUN_DATE', text_parameter, to_char(v_date,:GLOBAL.DATETIME_MASK));
		add_parameter (p_list_id, 'P_WORK_ENTITY', text_parameter, to_char(:C1.WORK_ENTITY));
		add_parameter (p_list_id, 'P_SEARCHGROUP_ID', text_parameter, to_char(:C1.ADTENTITY_SEARCHGROUP_ID));
		add_parameter (p_list_id, 'P_CALC_TOTALS_FLAG', text_parameter, :C1.CALC_TOTALS_FLAG);
		add_parameter (p_list_id, 'P_DATETIME_MASK', text_parameter, :GLOBAL.DATETIME_MASK);
		add_parameter (p_list_id, 'DESTYPE', text_parameter, :C1.CHECK1);
		add_parameter (p_list_id, 'PARAMFORM', text_parameter, 'NO');
		add_parameter (p_list_id, 'RECURSIVE_LOAD', text_parameter, 'NO'); -- La 10/1/02 avoids ORA-6508 when running report 1st time
		add_parameter (p_list_id, 'P_HOSPITAL_ID',text_parameter,TO_CHAR(get_hospital_id(TO_NUMBER(:GLOBAL.CURRENT_WORK_ENTITY)))); 		
		-- JB 20/02/04 Added visible check
		IF GET_ITEM_PROPERTY('C1.WORK_ID_TYPES', VISIBLE) = 'TRUE' THEN
			add_parameter (p_list_id, 'P_WORK_ID_TYPE',TEXT_PARAMETER,:c1.work_id_types);
		ELSE
			add_parameter (p_list_id, 'P_WORK_ID_TYPE',TEXT_PARAMETER,'RAISED');
		END IF;
		add_parameter (p_list_id, 'P_SITE_CODE',TEXT_PARAMETER,:c1.site_code);
		add_parameter (p_list_id, 'P_TITLE', text_parameter, :TEXT.BPTR_TITLE);
		add_parameter (p_list_id, 'P_BPTR_1', text_parameter, :text.BPTR_1);
		add_parameter (p_list_id, 'P_BPTR_2', text_parameter, :text.BPTR_2);
		add_parameter (p_list_id, 'P_BPTR_3', text_parameter, :text.BPTR_3);
		add_parameter (p_list_id, 'P_BPTR_4', text_parameter, :text.BPTR_4);
		add_parameter (p_list_id, 'P_BPTR_5', text_parameter, :text.BPTR_5);
		add_parameter (p_list_id, 'P_BPTR_6', text_parameter, :text.BPTR_6);
		add_parameter (p_list_id, 'P_BPTR_7', text_parameter, :text.BPTR_7);
		add_parameter (p_list_id, 'P_BPTR_8', text_parameter, :text.BPTR_8);
		add_parameter (p_list_id, 'P_BPTR_9', text_parameter, :text.BPTR_9);
		add_parameter (p_list_id, 'P_BPTR_10', text_parameter, :text.BPTR_10);
		add_parameter (p_list_id, 'P_BPTR_11', text_parameter, :text.BPTR_11);
		add_parameter (p_list_id, 'P_BPTR_12', text_parameter, :text.BPTR_12);
		add_parameter (p_list_id, 'P_BPTR_13', text_parameter, :text.BPTR_13);
		add_parameter (p_list_id, 'P_BPTR_14', text_parameter, :text.BPTR_14);
		add_parameter (p_list_id, 'P_BPTR_15', text_parameter, :text.BPTR_15);
		add_parameter (p_list_id, 'P_BPTR_16', text_parameter, :text.BPTR_16);
		add_parameter (p_list_id, 'P_BPTR_17', text_parameter, :text.BPTR_17);
		add_parameter (p_list_id, 'P_BPTR_18', text_parameter, :text.BPTR_18);
		add_parameter (p_list_id, 'P_BPTR_19', text_parameter, :text.BPTR_19);
		add_parameter (p_list_id, 'P_BPTR_20', text_parameter, :text.BPTR_20);
		add_parameter (p_list_id, 'P_BPTR_21', text_parameter, :text.BPTR_21);
		add_parameter (p_list_id, 'P_BPTR_22', text_parameter, :text.BPTR_22);
		add_parameter (p_list_id, 'P_BPTR_23', text_parameter, :text.BPTR_23);
		add_parameter (p_list_id, 'P_BPTR_24', text_parameter, :text.BPTR_24);
		add_parameter (p_list_id, 'P_BPTR_25', text_parameter, :text.BPTR_25);
		add_parameter (p_list_id, 'P_BPTR_26', text_parameter, :text.BPTR_26);
		add_parameter (p_list_id, 'P_BPTR_27', text_parameter, :text.BPTR_27);
		add_parameter (p_list_id, 'P_BPTR_28', text_parameter, :text.BPTR_28);
		add_parameter (p_list_id, 'P_BPTR_29', text_parameter, :text.BPTR_29);
		add_parameter (p_list_id, 'P_BPTR_30', text_parameter, :text.BPTR_30);

    if :c1.check1 not in ('SCREEN','PREVIEW') then
      run_product(reports,'BEDSTATE',synchronous,batch,filesystem,p_list_id,null);

    else
      run_product(reports,'BEDSTATE',synchronous,runtime,filesystem,p_list_id,null);

    end if;
   

END;
