--
-- date		by		change
-- 26/01/2000	Steve		Created
--
BEGIN

  IF show_lov ('ADTSEARCHGROUP_LOV') THEN

	IF :PARAMETER.P_LOV_CODE = 0 THEN
	  -- call ADTSRGRP form to add/modify ADT search groups

		oasis_call_form_v2 ('ADTSRGRP', NO_HIDE, DO_REPLACE, NO_QUERY_ONLY, 'TRUE');

	ELSE

		:C1.SEARCHGROUP_USER_CODE := :PARAMETER.P_LOV_USER_CODE;
		:C1.SEARCHGROUP_DESC := :PARAMETER.P_LOV_DESCRIPTION;
		:C1.ADTENTITY_SEARCHGROUP_ID := :PARAMETER.P_LOV_CODE;

		oasis_number_set_item_property ('C1.SEARCHGROUP_USER_CODE',
			ITEM_IS_VALID, PROPERTY_ON);

	END IF;

  END IF;

END;
